<ORGANIZATION>
    <USER TABLE_NAME="MF_ORG_USER" IS_ENABLE="true">
    	<ID COLNAME="USER_ID"/>
    	<LOGONNAME COLNAME="LOGON_NAME"/>
    	<PASSWORD COLNAME="PASSWORD"/>
    	<USERNAME COLNAME="USER_NAME"/>    	
    	<EMAILNAME COLNAME="EMAIL"/>
    	<DESC COLNAME=""/>
    	<ADDRESS COLNAME=""/>
    	<MOBILENUMBER COLNAME=""/>
    	<RTXNUMBER COLNAME=""/>
    	<HOMENUMBER COLNAME=""/>
    	<OFFICENUMBER COLNAME=""/>
    	<LEVEL COLNAME=""/>
    	<ORGID COLNAME="ORG_ID"/>
    	<ISABSENT COLNAME="IS_DELETED"/>
    </USER>
    <ROLE TABLE_NAME="MF_ORG_ROLE"  IS_ENABLE="true" ISTREE="true">
        <ID COLNAME="ROLE_ID"/>
        <NAME COLNAME="ROLE_NAME"/>
        <DESC COLNAME="ROLE_DESC"/>
        <ORDER COLNAME="ROLE_INDEX"/>
        <TYPEVALUE COLNAME="IS_ROLE_TYPE" DEFAULTVALUE="0" Type="Number"/>
        <PARENTID COLNAME="PARENT_ID" DEFAULTVALUE="mf_root"/>
    </ROLE>
    

    <DEP TABLE_NAME="WF_V_DEP"  IS_ENABLE="true" >
        <ID COLNAME="SEQUENCE_ID"/>
        <NAME COLNAME="DEP_NAME"/>
        <ORDER COLNAME="DEP_INDEX"/>
        <PARENTID COLNAME="PARENT_ID" DEFAULTVALUE="root"/>
    </DEP>

 
    <USERROLE TABLE_NAME="MF_USER_HAS_ROLE"  IS_ENABLE="true">
        <USERID COLNAME="USER_ID"/>
        <ROLEID COLNAME="ROLE_ID"/>
        <LIMITDEPID COLNAME="LIMIT_DEPID" DEFAULTVALUE="mf_root"/>
    </USERROLE>
    <USERDEP TABLE_NAME="MF_USER_IN_ORG"  IS_ENABLE="true">
        <USERID COLNAME="USER_ID"/>
        <DEPID COLNAME="DEP_ID"/>
        <ORDER COLNAME="USER_INDEX"/>
    </USERDEP>
</ORGANIZATION>
