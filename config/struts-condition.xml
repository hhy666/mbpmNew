<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
   
<struts>
	 <!-- 条件设置Action -->
	<package name="com.matrix.form.admin.custom.condition.action" extends="globalPackage" namespace="/condition">
		<action name="condition_*" class="conditionAction" method="{1}">
    		<result name="condition">/form/admin/custom/condition/conditionSet.jsp</result>
    		<result name="getYMDW">/form/admin/custom/condition/getYMDW.jsp</result>
    		<result name="daySub">/form/admin/custom/condition/daySub.jsp</result>
    		<result name="advancedSetPage">/form/admin/custom/condition/advancedSet.jsp</result>
    	</action>
	</package> 
</struts>